#First exercise 

my_list = [i ** 2 for i in range(1, 11)]

my_file = open("output.txt", "w")

# Add your code below!
for i in my_list:
  my_file.write(str(i) + "\n")

my_file.close()

#Second exercise
with open("text.txt","w") as my_file:
  my_file.write("test")

if my_file.closed == "False":
  my_file.close()

print(my_file.closed)